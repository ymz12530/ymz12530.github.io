<body>
    <div class="container">
        <h1>MTFæµ‹è¯•å€¼æå–å·¥å…·</h1>
        
        <!-- ä¸Šä¼ åŒºåŸŸ -->
        <div class="upload-area" id="uploadArea">
            <input type="file" id="fileInput" accept=".log,.txt">
            <div class="upload-icon">ğŸ“¤</div>
            <div class="upload-text">ç‚¹å‡»æˆ–æ‹–æ‹½ä¸Šä¼ .log/.txtæ—¥å¿—æ–‡ä»¶</div>
            <div class="upload-tip">æ”¯æŒå•ä¸ªæ–‡ä»¶ä¸Šä¼ ï¼Œè‡ªåŠ¨æå–SNåŠæ‰€æœ‰MTFæµ‹è¯•å€¼</div>
        </div>
        
        <!-- é”™è¯¯æç¤º -->
        <div class="error-message" id="errorMessage" style="display: none;"></div>
        
        <!-- ç»“æœå±•ç¤ºåŒºåŸŸ -->
        <div class="result-area" id="resultArea">
            <div class="sn-info">
                <span class="sn-label">è®¾å¤‡SNï¼š</span>
                <span class="sn-value" id="snValue"></span>
            </div>
            
            <table>
                <thead>
                    <tr>
                        <<th>æµ‹è¯•åºå·</</th>
                        <<th>æµ‹è¯•æ—¶é—´</</th>
                        <<th>MTFæµ‹è¯•å€¼ï¼ˆ9ä¸ªå‚æ•°ï¼‰</</th>
                        <<th>æµ‹è¯•çŠ¶æ€</</th>
                    </tr>
                </thead>
                <tbody id="mtfTableBody">
                    <!-- æµ‹è¯•æ•°æ®å°†åŠ¨æ€æ’å…¥è¿™é‡Œ -->
                </tbody>
            </table>
            
            <button class="download-btn" id="downloadBtn">ä¸‹è½½æå–ç»“æœï¼ˆCSVæ ¼å¼ï¼‰</button>
        </div>
    </div>

    <script>
        // DOMå…ƒç´ 
        const uploadArea = document.getElementById('uploadArea');
        const fileInput = document.getElementById('fileInput');
        const errorMessage = document.getElementById('errorMessage');
        const resultArea = document.getElementById('resultArea');
        const snValue = document.getElementById('snValue');
        const mtfTableBody = document.getElementById('mtfTableBody');
        const downloadBtn = document.getElementById('downloadBtn');
        
        // ä¸Šä¼ åŒºåŸŸç‚¹å‡»äº‹ä»¶
        uploadArea.addEventListener('click', () => {
            fileInput.click();
        });
        
        // æ‹–æ‹½äº‹ä»¶
        uploadArea.addEventListener('dragover', (e) => {
            e.preventDefault();
            uploadArea.style.background = '#e8f4f8';
        });
        
        uploadArea.addEventListener('dragleave', () => {
            uploadArea.style.background = 'transparent';
        });
        
        uploadArea.addEventListener('drop', (e) => {
            e.preventDefault();
            uploadArea.style.background = 'transparent';
            if (e.dataTransfer.files.length > 0) {
                fileInput.files = e.dataTransfer.files;
                handleFileUpload(fileInput.files[0]);
            }
        });
        
        // æ–‡ä»¶é€‰æ‹©äº‹ä»¶
        fileInput.addEventListener('change', () => {
            if (fileInput.files.length > 0) {
                handleFileUpload(fileInput.files[0]);
            }
        });
        
        // å¤„ç†æ–‡ä»¶ä¸Šä¼ 
        function handleFileUpload(file) {
            // éªŒè¯æ–‡ä»¶ç±»å‹
            if (!file.name.endsWith('.log') && !file.name.endsWith('.txt')) {
                showError('è¯·ä¸Šä¼ .logæˆ–.txtæ ¼å¼çš„æ—¥å¿—æ–‡ä»¶');
                return;
            }
            
            const reader = new FileReader();
            
            reader.onload = (e) => {
                const logContent = e.target.result;
                const { sn, mtfList } = parseLogContent(logContent);
                
                if (!sn) {
                    showError('æœªä»æ—¥å¿—ä¸­æå–åˆ°è®¾å¤‡SN');
                    return;
                }
                
                if (mtfList.length === 0) {
                    showError('æœªä»æ—¥å¿—ä¸­æå–åˆ°MTFæµ‹è¯•å€¼');
                    return;
                }
                
                // å±•ç¤ºç»“æœ
                showResult(sn, mtfList);
            };
            
            reader.onerror = () => {
                showError('æ–‡ä»¶è¯»å–å¤±è´¥ï¼Œè¯·é‡è¯•');
            };
            
            reader.readAsText(file, 'UTF-8');
        }
        
        // è§£ææ—¥å¿—å†…å®¹
        function parseLogContent(content) {
            const lines = content.split('\n');
            let sn = '';
            const mtfList = [];
            let testIndex = 0;
            
            // æå–SNï¼ˆåŒ¹é… ReadSN = XXXX æ ¼å¼ï¼‰
            const snRegex = /ReadSN\s*=\s*(\S+)/;
            // æå–MTFæµ‹è¯•å€¼ï¼ˆåŒ¹é… message: åé¢çš„æ•°å€¼åˆ—è¡¨ï¼‰
            const mtfRegex = /message:\s*(.+)/;
            // æå–æ—¶é—´ï¼ˆåŒ¹é… [INFO ] å‰é¢çš„æ—¶é—´æˆ³ï¼‰
            const timeRegex = /^(\d{4}-\d{2}-\d{2}\s+\d{2}:\d{2}:\d{2}\.\d{3})/;
            
            lines.forEach(line => {
                // æå–SN
                if (!sn) {
                    const snMatch = line.match(snRegex);
                    if (snMatch && snMatch[1]) {
                        sn = snMatch[1];
                    }
                }
                
                // æå–MTFå€¼
                const mtfMatch = line.match(mtfRegex);
                if (mtfMatch && mtfMatch[1]) {
                    testIndex++;
                    const mtfContent = mtfMatch[1].trim();
                    let status = 'success';
                    let mtfValues = [];
                    
                    // è§£æçŠ¶æ€å’Œæ•°å€¼
                    if (mtfContent.includes('è¯»å–æœ¬åœ°å›¾ç‰‡å¤±è´¥')) {
                        status = 'error';
                        mtfValues = ['è¯»å–å›¾ç‰‡å¤±è´¥'];
                    } else if (mtfContent.includes('-inf')) {
                        status = 'warning';
                        mtfValues = mtfContent.split(',').filter(val => val && val !== '-inf');
                    } else {
                        // æå–æ•°å­—ï¼ˆåŒ…å«æ­£è´Ÿå·å’Œå°æ•°ç‚¹ï¼‰
                        mtfValues = mtfContent.split(',').filter(val => /^-?\d+(\.\d+)?$/.test(val.trim()));
                    }
                    
                    // æå–å½“å‰è¡Œçš„æ—¶é—´
                    let time = 'æœªçŸ¥æ—¶é—´';
                    const timeMatch = line.match(timeRegex);
                    if (timeMatch && timeMatch[1]) {
                        time = timeMatch[1];
                    }
                    
                    mtfList.push({
                        index: testIndex,
                        time: time,
                        values: mtfValues,
                        status: status
                    });
                }
            });
            
            return { sn, mtfList };
        }
        
        // æ˜¾ç¤ºé”™è¯¯ä¿¡æ¯
        function showError(msg) {
            errorMessage.textContent = msg;
            errorMessage.style.display = 'block';
            resultArea.style.display = 'none';
        }
        
        // æ˜¾ç¤ºç»“æœ
        function showError(msg) {
            errorMessage.textContent = msg;
            errorMessage.style.display = 'block';
            resultArea.style.display = 'none';
        }
        
        // æ˜¾ç¤ºç»“æœ
        function showResult(sn, mtfList) {
            errorMessage.style.display = 'none';
            resultArea.style.display = 'block';
            
            // è®¾ç½®SNå€¼
            snValue.textContent = sn;
            
            // æ¸…ç©ºè¡¨æ ¼
            mtfTableBody.innerHTML = '';
            
            // å¡«å……è¡¨æ ¼
            mtfList.forEach(item => {
                const row = document.createElement('tr');
                
                // çŠ¶æ€æ–‡æœ¬å’Œæ ·å¼
                const statusMap = {
                    success: { text: 'æ­£å¸¸', class: 'status-success' },
                    warning: { text: 'æ— æ•ˆå€¼', class: 'status-warning' },
                    error: { text: 'å¤±è´¥', class: 'status-error' }
                };
                const statusInfo = statusMap[item.status];
                
                // æ ¼å¼åŒ–MTFå€¼
                const mtfText = item.values.length > 0 
                    ? item.values.join(', ') 
                    : 'æ— æœ‰æ•ˆæ•°å€¼';
                
                row.innerHTML = `
                    <td>${item.index}</td>
                    <td>${item.time}</td>
                    <td class="mtf-value">${mtfText}</td>
                    <td><span class="${statusInfo.class}">${statusInfo.text}</span></td>
                `;
                
                mtfTableBody.appendChild(row);
            });
            
            // ç»‘å®šä¸‹è½½äº‹ä»¶
            downloadBtn.onclick = () => {
                downloadCSV(sn, mtfList);
            };
        }
        
        // ä¸‹è½½CSVæ–‡ä»¶
        function downloadCSV(sn, mtfList) {
            // æ„å»ºCSVå†…å®¹
            let csvContent = 'æµ‹è¯•åºå·,æµ‹è¯•æ—¶é—´,MTFæµ‹è¯•å€¼,æµ‹è¯•çŠ¶æ€\n';
            
            const statusTextMap = {
                success: 'æ­£å¸¸',
                warning: 'æ— æ•ˆå€¼',
                error: 'å¤±è´¥'
            };
            
            mtfList.forEach(item => {
                const mtfValues = item.values.join('; ');
                const statusText = statusTextMap[item.status];
                // å¤„ç†CSVä¸­çš„ç‰¹æ®Šå­—ç¬¦ï¼ˆé€—å·ã€å¼•å·ï¼‰
                const row = [
                    item.index,
                    `"${item.time}"`,
                    `"${mtfValues}"`,
                    `"${statusText}"`
                ].join(',');
                csvContent += row + '\n';
            });
            
            // åˆ›å»ºBlobå¹¶ä¸‹è½½
            const blob = new Blob([csvContent], { type: 'text/csv;charset=utf-8;' });
            const url = URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = `${sn}_MTFæµ‹è¯•å€¼æå–ç»“æœ_${new Date().toLocaleDateString()}.csv`;
            a.click();
            URL.revokeObjectURL(url);
        }
    </script>
